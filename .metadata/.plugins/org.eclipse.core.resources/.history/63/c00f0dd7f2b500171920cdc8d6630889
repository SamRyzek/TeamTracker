$(document).ready(function() {
    console.log("loaded file");
    printTeam();
});

var printTeam = function(){ 
$.ajax({
    type: 'GET',
    url: 'http://localhost:8080/TeamRest/rest/teams',
    dataType: "json"
	})
	.done(function(data, status){
        console.log(data);
    })
    .fail(function(xhr, status, error){
        console.error("Error");
    });
};